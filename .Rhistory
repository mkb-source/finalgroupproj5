ses_weights <- function(alpha) {
i <- 0:4
weights <- alpha * (1 - alpha) ^ i
plot(i, weights, type = 'l', main = paste('alpha = ', alpha))
text(weights, x = i, y = weights, col = 'red')
}
ses_weights(alpha = 0.2)
ses_weights(alpha = 0.8)
